data Maybe (a : Type) where
| Just a
| Nothing

def fromMaybe {a} (fallback : a) (m : Maybe a) -> a do
  match m with
  | Just x -> x
  | Nothing -> fallback

def map {a, b} (f : a -> b) (m : Maybe a)  -> Maybe b do 
  match m with 
  | Just x -> Just (f x)
  | Nothing -> Nothing

def main () -> Int do
  fromMaybe 0 (map (\x -> x + 1) (Just 0))
